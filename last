###END BDTMYPRC
$$$BEG PDOBKSTR
T$P=STRIP(WORD(SRC.D#####Z,1),"B")
T$T=SUBSTR(SRC.D#####Z,69,1)
IF T$T="E" THEN T$T="D"
E$SN="TTE"T$T".TSK.F.R320."T$P".PARMLIB(STAR*)"
ADDRESS ISPEXEC "VIEW DATASET('"E$SN"')"
###END PDOBKSTR
$$$BEG PDBKSI1
CGE5      A{DP}                                                    YE320
CGE7      A{DS}                                                    YE320
TSDHA02   A{DS}                                                    YE320
TSDHA03   A{DS}                                                    YE320
TSDHA11   A{DS}                                                    XE320
TSDHA12   A{DS}                                                    XE320
TSDQA11   A{DP}                                                    YE320
###END PDBKSI1
