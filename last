     CEDF(YES)
     DATALOCATION(BELOW)
     EXECKEY(CICS)
     CONCURRENCY(QUASIRENT)
     API(CICSAPI)
     DYNAMIC(NO)
     EXECUTIONSET(FULLAPI)
     JVM(NO)
     DEFINETIME(14/01/16 07:17:12)
     CHANGETIME(14/01/21 10:33:29)
     CHANGEUSRID(TCEM)
     CHANGEAGENT(CSDAPI)
     CHANGEAGREL(0680)

 DELETE GROUP(CEM) PROGRAM(PGMAM24)
###END RDOPGM24
