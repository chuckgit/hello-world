$$$BEG PONGCNC
000100
000200***************************************************************
000300* V1.0   20110722
       this is a copied version and is not the maintained source
       (these statements are meant to cause the compile to fail)
       see TTED.TSK.D.TCEM.COB(PONGCNC) for the maintained version
000400*****************************************************************
000500*    CICS COBOL Program Reads Commarea or CnC
000600*    makes this know by calling PONG with the message
000700*    also if cnc, itterate thru the conainer name and lng
000800*    appends this list of names and lng to a new PONGRET
000900*    container (or returns PONG msg in commarea).
001000*****************************************************************
001100
001200 IDENTIFICATION DIVISION.
001300 PROGRAM-ID. PONGCNC.
001400 ENVIRONMENT DIVISION.
001500 DATA DIVISION.
001600 WORKING-STORAGE SECTION.
001700 77  WS-BEGIN   VALUE '<<<WS BEGINS>>>' PIC X(15).
001800 77  WS-PROGRAM-ID VALUE SPACES    PIC X(9).
001900 77  WS-COMPILE-DT                 PIC X(21).
002000*    SET THIS VALUE TO BASE OS. USE TO PERF DATA CONV
002100*    SET ZOS OR AIX
002200 77  WS-OS          VALUE 'ZOS'    PIC X(3).
002300 77  MSGOUT         VALUE SPACES   PIC X(32760).
002400 77  MSGOUT-LNG     VALUE ZERO     PIC S9(4) COMP.
002500 77  MSGOUT-FLNG     VALUE ZERO     PIC S9(8) COMP.
002600 77  TDQOUT         VALUE SPACES   PIC X(120).
002700 77  TDQOUT-LNG     VALUE ZERO     PIC S9(4) COMP.
002800 77  MSG-LNG-D      VALUE ZERO     PIC Z(5) DISPLAY.
002900 77  WS-ABEND       VALUE 'PCNC'   PIC X(4).
003000 77  WS-RC          VALUE ZERO     PIC Z(4) DISPLAY.
003100 77  WS-SP          VALUE SPACE    PIC X(1).
003200 77  CALEN-SV                      PIC S9(8) BINARY.
003300 77  P                             PIC S9(9) BINARY.
003400 01  WORK-AREAS.
003500     05  RESP1      VALUE ZERO        PIC S9(8) COMP.
003600     05  RESP2      VALUE ZERO        PIC S9(8) COMP.
003700     05  CH-NM                        PIC X(16).
003800     05  CN-RET-NM                    PIC X(16).
003900     05  CN-NM                        PIC X(16).
004000     05  CN-BRS-TKN VALUE ZERO        PIC S9(8) COMP.
004100     05  CN-LNG  VALUE ZERO           PIC S9(8) COMP.
004200     05  CN-LNG-D VALUE ZERO          PIC Z9(8) DISPLAY.
004300     05  CN-CT   VALUE ZERO           PIC S9(8) COMP.
004400     05  CN-PTR                       PIC S9(8) COMP.
004500     05  CALL-PGM                     PIC X(8).
004600 77  WS-END     VALUE '<<<WS----END>>>' PIC X(15).
004700 LINKAGE SECTION.
004800
004900 01 DFHCOMMAREA.
005000   02 CA-IN.
005100    03 CONN-DATA                    PIC X(1)
005200       OCCURS 0 TO 32767 DEPENDING ON CALEN-SV.
005300
005400 PROCEDURE DIVISION.
005500     EXEC CICS HANDLE ABEND LABEL(DO-ABEND)
005600          END-EXEC.
005700     EXEC CICS ASSIGN PROGRAM(WS-PROGRAM-ID)
005800          END-EXEC.
005900     MOVE WHEN-COMPILED TO WS-COMPILE-DT.
006000*    MOVE WS-PROGRAM-ID TO MSGOUT.
006100     MOVE 1 TO P.
006200     STRING
006300      WS-PROGRAM-ID, WS-SP
006400      DELIMITED BY SPACE
006500      INTO MSGOUT WITH POINTER P
006600      END-STRING.
006700     STRING WS-PROGRAM-ID
006800      DELIMITED BY SPACE
006900      '-CNRETNM', SPACES
007000      DELIMITED BY SIZE
007100      INTO CN-RET-NM
007200      END-STRING.
007300     STRING MSGOUT DELIMITED BY SPACE
007400      INTO MSGOUT WITH POINTER P
007500      END-STRING.
007600     MOVE 'PONG'     TO CALL-PGM.
007700     MOVE LENGTH OF TDQOUT TO TDQOUT-LNG.
007800     MOVE LENGTH OF MSGOUT TO MSGOUT-LNG, MSGOUT-FLNG.
007900     IF EIBCALEN NOT = 0 THEN
008000       MOVE EIBCALEN TO MSG-LNG-D
008100       MOVE EIBCALEN TO CALEN-SV
008200       IF EIBCALEN > MSGOUT-LNG THEN
008300        STRING WS-SP
008400         DELIMITED BY SPACE
008500         'COMMAREA OVERSIZED'
008600         DELIMITED BY SIZE
008700         INTO MSGOUT WITH POINTER P
008800         END-STRING
008900        END-IF
009000       STRING 'CAL:<'
009100        DELIMITED BY SPACE
009200        MSG-LNG-D, '>CA:<'
009300        DFHCOMMAREA
009400        DELIMITED BY SIZE
009500        INTO MSGOUT WITH POINTER P
009600        END-STRING
009700       EXEC CICS LINK PROGRAM(CALL-PGM)
009800        COMMAREA(MSGOUT)
009900        LENGTH(MSGOUT-LNG)
010000        RESP(RESP1)
010100        RESP2(RESP2)
010200        END-EXEC
010300       GO TO RET-WITH-CA
010400      END-IF.
010500 DO-CNC.
010600     EXEC CICS ASSIGN CHANNEL(CH-NM)
010700      RESP(RESP1)
010800      RESP2(RESP2)
010900      END-EXEC.
011000     IF RESP1 NOT = DFHRESP(NORMAL) THEN
011100        STRING
011200         WS-SP
011300         DELIMITED BY SPACE
011400         'CHANNEL:<', CH-NM, '>'
011500         DELIMITED BY SPACE
011600         'BAD CHANNEL RESP. BYE.'
011700         DELIMITED BY SIZE
011800         INTO TDQOUT
011900         END-STRING
012000        GO TO DO-TDQ
012100        END-IF.
012200     IF CH-NM = SPACE THEN
012300        STRING
012400         ' CHANNEL:<'
012500         DELIMITED BY SPACE
012600         'CHANNEL NAME SPACES AND NO CA. BYE'
012700         DELIMITED BY SIZE
012800         INTO MSGOUT WITH POINTER P
012900         END-STRING
013000        STRING
013100         MSGOUT
013200         DELIMITED BY SIZE
013300         INTO TDQOUT
013400         END-STRING
013500        GO TO DO-TDQ
013600        END-IF.
013700     STRING
013800      ' CHANNEL:<', CH-NM, '>'
013900      DELIMITED BY SPACE
014000      INTO MSGOUT WITH POINTER P
014100      END-STRING.
014200     EXEC CICS STARTBROWSE CONTAINER
014300      CHANNEL(CH-NM)
014400      BROWSETOKEN(CN-BRS-TKN)
014500      RESP(RESP1)
014600      RESP2(RESP2)
014700      END-EXEC.
014800     IF RESP1 NOT = DFHRESP(NORMAL) THEN
014900        STRING
015000         WS-SP
015100         DELIMITED BY SPACE
015200         '> NO CONTAINERS. BYE.'
015300         DELIMITED BY SIZE
015400         INTO MSGOUT WITH POINTER P
015500         END-STRING
015600        GO TO CN-LOOP-XIT
015700        END-IF.
015800 CN-LOOP.
015900     EXEC CICS GETNEXT CONTAINER(CN-NM)
016000      BROWSETOKEN(CN-BRS-TKN)
016100      RESP(RESP1)
016200      RESP2(RESP2)
016300      END-EXEC.
016400     IF RESP1 = DFHRESP(END) THEN
016500        GO TO CN-LOOP-XIT
016600        END-IF.
016700     IF RESP1 NOT = DFHRESP(NORMAL) THEN
016800        MOVE CN-CT TO CN-LNG-D
016900        STRING
017000         WS-SP
017100         DELIMITED BY SPACE
017200         'BAD CONTAINER AT COUNT+1:', CN-LNG-D
017300         DELIMITED BY SIZE
017400         INTO MSGOUT WITH POINTER P
017500         END-STRING
017600        GO TO CN-BYP-INQ
017700        END-IF.
017800     EXEC CICS GET CONTAINER(CN-NM)
017900      FLENGTH(CN-LNG)
018000      SET(CN-PTR)
018100      RESP(RESP1)
018200      RESP2(RESP2)
018300      END-EXEC.
018400     IF RESP1 NOT = DFHRESP(NORMAL) THEN
018500        STRING
018600         ' CONTAINER:<', CN-NM, ':'
018700         DELIMITED BY SPACE
018800         'BAD CONTAINER. '
018900         DELIMITED BY SIZE
019000         INTO MSGOUT WITH POINTER P
019100         END-STRING
019200        ELSE
019300         MOVE CN-LNG TO CN-LNG-D
019400         STRING
019500         ' CONTAINER:<', CN-NM, ':'
019600          DELIMITED BY SPACE
019700          CN-LNG-D, '> '
019800          DELIMITED BY SIZE
019900          INTO MSGOUT WITH POINTER P
020000          END-STRING
020100         END-IF.
020200 CN-BYP-INQ.
020300     ADD 1 TO CN-CT.
020400     IF CN-CT > 99 THEN
020500        STRING
020600         WS-SP
020700         DELIMITED BY SPACE
020800         'CONTAINER COUNT GT 99. BYE'
020900         DELIMITED BY SIZE
021000         INTO MSGOUT WITH POINTER P
021100         END-STRING
021200        GO TO CN-LOOP-XIT
021300        END-IF.
021400        GO TO CN-LOOP.
021500 CN-LOOP-XIT.
021600     EXEC CICS ENDBROWSE CONTAINER
021700        BROWSETOKEN(CN-BRS-TKN)
021800        RESP(RESP1)
021900        RESP2(RESP2)
022000        END-EXEC.
022100     IF CN-CT = 0 THEN
022200        STRING
022300         '!!! '
022400         DELIMITED BY SPACE
022500         'NO CONTAINERS IN CHANNEL. '
022600         DELIMITED BY SIZE
022700         INTO MSGOUT WITH POINTER P
022800         END-STRING
022900        END-IF.
023000     EXEC CICS LINK PROGRAM(CALL-PGM)
023100         COMMAREA(MSGOUT)
023200         LENGTH(MSGOUT-LNG)
023300         RESP(RESP1)
023400         RESP2(RESP2)
023500         END-EXEC.
023600     GO TO RET-WITH-CNC.
023700 RET-WITH-CA.
023800     STRING
023900      MSGOUT
024000      DELIMITED BY SIZE
024100      INTO DFHCOMMAREA
024200      END-STRING.
024300     GO TO DO-RETURN.
024400 RET-WITH-CNC.
024500     EXEC CICS PUT CONTAINER(CN-RET-NM)
024600      CHANNEL(CH-NM)
024700      FROM(MSGOUT)
024800      FLENGTH(MSGOUT-FLNG)
024900      DATATYPE(DFHVALUE(CHAR))
025000      RESP(RESP1)
025100      RESP2(RESP2)
025200      END-EXEC.
025300     IF RESP1 NOT = DFHRESP(NORMAL) THEN
025400        STRING
025500         WS-SP
025600         DELIMITED BY SPACE
025700         '<', CH-NM, '><', CN-RET-NM, '>'
025800         DELIMITED BY SPACE
025900         'PUT CHANNEL RESP. BYE.'
026000         DELIMITED BY SIZE
026100         INTO MSGOUT WITH POINTER P
026200         END-STRING
026300        STRING
026400         MSGOUT
026500         DELIMITED BY SIZE
026600         INTO TDQOUT
026700         END-STRING
026800        GO TO DO-TDQ
026900        END-IF.
027000     GO TO DO-RETURN.
027100 DO-TDQ.
027200     EXEC CICS WRITEQ TD QUEUE('CSSL')
027300      FROM (TDQOUT) LENGTH(TDQOUT-LNG)
027400      RESP(RESP1)
027500      RESP2(RESP2)
027600      END-EXEC.
027700     GO TO DO-RETURN.
027800 DO-ABEND.
027900       EXEC CICS ABEND ABCODE(WS-ABEND)
028000               END-EXEC.
028100       MOVE 255 TO WS-RC.
028200 DO-RETURN.
028300     EXEC CICS RETURN END-EXEC.
###END PONGCNC
$$$BEG LINKLU62
 DEFINE CONNECTION(CGE5) GROUP(CEMLU62)
     NETNAME(CGE5)
     ACCESSMETHOD(VTAM)
     PROTOCOL(APPC)
     SINGLESESS(NO)
     DATASTREAM(USER)
     RECORDFORMAT(U)
     QUEUELIMIT(NO)
     MAXQTIME(NO)
     AUTOCONNECT(YES)
     INSERVICE(YES)
     ATTACHSEC(IDENTIFY)
     BINDSECURITY(NO)
     USEDFLTUSER(YES)
